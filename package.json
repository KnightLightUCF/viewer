    "aframe": "^1.0.4",
    "aframe-environment-component": "^2.0.0",
    "aframe-glow": "^1.0.1",
    "aframe-look-at-component": "^1.0.0",
    "aframe-meshline-component": "^0.3.1",
    "aframe-orbit-controls": "^1.3.0",
    "clsx": "^1.1.1",
    "color": "^3.1.2",
    "delay": "^4.4.0",
    "denque": "^1.4.1",
    "electron-better-ipc": "^1.1.0",
    "electron-store": "^6.0.0",
    "electron-unhandled": "^3.0.2",
    "electron-util": "^0.14.2",
    "electron-window-state": "^5.0.3",
    "esm": "^3.2.25",
