    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.2",
    "css-loader": "^4.0.0",
    "dotenv-webpack": "^2.0.0",
    "electron": "^9.1.2",
    "electron-builder": "^22.8.0",
    "eslint": "^7.5.0",
    "eslint-config-xo-react": "^0.23.0",
