{
  "name": "@collmot/skybrush-viewer",
  "productName": "Skybrush Viewer",
  "version": "1.0.0",
  "description": "Viewer for Skybrush drone shows",
  "main": "launcher.bundle.js",
  "scripts": {
    "bundle": "rimraf build && cross-env NODE_ENV=production webpack --mode=production --progress --config webpack/dist.config.js",
    "dist": "node scripts/deploy.js -p",
    "doc": "esdoc -c esdoc.json",
    "lint": "xo",
    "postinstall": "patch-package",
    "start": "webpack-dev-server --config webpack/browser.config.js --progress --colors",
    "start:electron": "webpack-dev-server --https --config webpack/electron.config.js --progress --colors",
    "start:public": "webpack-dev-server --config webpack/browser.config.js --progress --colors --host 0.0.0.0",
    "start:public:secure": "webpack-dev-server --https --config webpack/browser.config.js --progress --colors --host 0.0.0.0",
    "test": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git@git.collmot.com:collmot/skybrush-viewer.git"
  },
  "keywords": [],
  "author": "CollMot Robotics Ltd",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@babel/polyfill": "^7.10.4",
    "@collmot/layout-bmfont-text": "^1.3.5",
    "@material-ui/core": "^4.11.0",
